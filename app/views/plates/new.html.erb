<%= form_for(@plate, html: { class: "form-horizontal"}) do |f| %>
	<div class="control-group">
		<%= f.label :license, class: 'control-label' %>
		<div class="controls">
			<%= f.text_field :license %>
		</div>
	</div>
	<div class="control-group" >
    	<div class="controls">
      <%= f.submit 'Add', class: 'btn btn-primary'%>
    </div>
  </div>
<% end %>




